Cell[BoxData[
 RowBox[{
  RowBox[{"PostVarianceOptsbs", "[", "nph_", "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "PostShotVarOptRatio", ",", "PostShot1Variance", ",", "numPho", ",", 
      "PostShot1OptState", ",", "PostShot1Opt\[CapitalDelta]", ",", 
      "PostShot2Variance", ",", "PostShot2OptState", ",", "uncertainty"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"PostShotVarOptRatio", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"uncertainty", " ", "=", " ", 
          FractionBox[
           RowBox[{"(", 
            RowBox[{"num", "*", "\[Pi]"}], ")"}], "10"]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"Clear", "[", "\[CapitalDelta]", "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"numPho", "=", "nph"}], ";", "\[IndentingNewLine]", 
         RowBox[{"PostShot1Variance", " ", "=", " ", 
          RowBox[{"GetVariance", "[", 
           RowBox[{"1", ",", "numPho", ",", "\[CapitalDelta]"}], "]"}]}], ";",
          "\[IndentingNewLine]", 
         RowBox[{"(*", 
          RowBox[{
           RowBox[{
           "Find", " ", "variance", " ", "after", " ", "1", " ", "shot", " ", 
            "Variance", 
            RowBox[{"(", "\[CapitalDelta]", ")"}]}], ",", " ", 
           RowBox[{
           "plug", " ", "that", " ", "into", " ", "ExtractOptState", " ", 
            "to", " ", "return", " ", "the", " ", "optimized", " ", 
            "numerical", " ", "value", " ", "for", " ", "Variance"}]}], 
          "*)"}], "\[IndentingNewLine]", 
         RowBox[{"PostShot1OptState", " ", "=", " ", 
          RowBox[{"ExtractOptState", "[", 
           RowBox[{"uncertainty", ",", "PostShot1Variance"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"(*", 
          RowBox[{
          "Calculate", " ", "uncertainty", " ", "post", " ", "shot", " ", 
           "1"}], "*)"}], "\[IndentingNewLine]", 
         RowBox[{"PostShot1Opt\[CapitalDelta]", " ", "=", " ", 
          SqrtBox[
           RowBox[{"12", "*", 
            RowBox[{"PostShot1OptState", "[", 
             RowBox[{"[", "1", "]"}], "]"}]}]]}], ";", "\[IndentingNewLine]", 
         RowBox[{"(*", 
          RowBox[{
          "Replace", " ", "post", " ", "shot", " ", "1", " ", "optimal", " ", 
           "state", " ", "as", " ", "input", " ", "into", " ", "2", "nd", " ",
            "shot"}], "*)"}], "\[IndentingNewLine]", 
         RowBox[{"PostShot2Variance", " ", "=", " ", 
          RowBox[{"GetVariance", "[", 
           RowBox[{"1", ",", "numPho", ",", "PostShot1Opt\[CapitalDelta]"}], 
           "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"PostShot2OptState", " ", "=", " ", 
          RowBox[{"ExtractOptState", "[", 
           RowBox[{"PostShot1Opt\[CapitalDelta]", ",", "PostShot2Variance"}], 
           "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{"PostShot1OptState", ",", "PostShot2OptState"}], "}"}]}], 
        "\[IndentingNewLine]", 
        RowBox[{"(*", 
         FractionBox[
          RowBox[{"PostShot2OptState", "[", 
           RowBox[{"[", "1", "]"}], "]"}], 
          FractionBox[
           SuperscriptBox["uncertainty", "2"], "12"]], "*)"}], ",", 
        RowBox[{"{", 
         RowBox[{"num", ",", "1", ",", "10"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "PostShotVarOptRatio", "]"}], ";"}]}], 
   "]"}]}]], "Input",
